{
  "dfx": "0.29.0",
  "canisters": {
    "realm_backend": {
      "build": "python -m kybra realm_backend src/realm_backend/main.py",
      "candid": "src/realm_backend/realm_backend.did",
      "declarations": {
        "node_compatibility": true
      },
      "gzip": true,
      "metadata": [
        {
          "name": "candid:service"
        }
      ],
      "tech_stack": {
        "cdk": {
          "kybra": {}
        },
        "language": {
          "python": {}
        }
      },
      "type": "custom",
      "wasm": ".kybra/realm_backend/realm_backend.wasm"
    },
    "realm_frontend": {
      "source": [
        "src/realm_frontend/dist"
      ],
      "type": "assets",
      "workspace": "realm_frontend"
    },
    "realm_registry_frontend": {
      "source": [
        "src/realm_registry_frontend/dist"
      ],
      "type": "assets",
      "workspace": "realm_registry_frontend"
    },
    "internet_identity": {
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "frontend": {},
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "type": "custom",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz"
    },
    "ckbtc_ledger": {
      "type": "custom",
      "candid": "https://raw.githubusercontent.com/dfinity/ic/master/rs/rosetta-api/icrc1/ledger/ledger.did",
      "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/ic-icrc1-ledger.wasm.gz",
      "remote": {
        "id": {
          "ic": "mxzaz-hqaaa-aaaar-qaada-cai"
        }
      },
      "init_arg": "(variant { Init = record { token_name = \"ckBTC\"; token_symbol = \"ckBTC\"; minting_account = record { owner = principal \"$(dfx identity get-principal)\" }; initial_balances = vec {}; metadata = vec {}; transfer_fee = 10; archive_options = record { trigger_threshold = 2000; num_blocks_to_archive = 1000; controller_id = principal \"$(dfx identity get-principal)\" }; feature_flags = opt record { icrc2 = true }; } })"
    },
    "ckbtc_minter": {
      "type": "custom",
      "candid": "https://raw.githubusercontent.com/dfinity/ic/master/rs/bitcoin/ckbtc/minter/ckbtc_minter.did",
      "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/ic-ckbtc-minter.wasm.gz",
      "remote": {
        "id": {
          "ic": "mqygn-kiaaa-aaaar-qaadq-cai"
        }
      },
      "init_arg": "(variant { Init = record { btc_network = variant { Regtest }; ledger_id = principal \"$(dfx canister id ckbtc_ledger)\"; ecdsa_key_name = \"dfx_test_key\"; retrieve_btc_min_amount = 5_000; max_time_in_queue_nanos = 420_000_000_000; min_confirmations = opt 12; mode = variant { GeneralAvailability }; kyt_fee = opt 1_000; kyt_principal = null; } })"
    },
    "ckbtc_kyt": {
      "type": "custom",
      "candid": "https://raw.githubusercontent.com/dfinity/ic/master/rs/bitcoin/ckbtc/kyt/kyt.did",
      "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/ic-ckbtc-kyt.wasm.gz",
      "remote": {
        "id": {
          "ic": "pjihx-aaaaa-aaaar-qaaka-cai"
        }
      },
      "init_arg": "(variant { InitArg = record { minter_id = principal \"$(dfx canister id ckbtc_minter)\"; maintainers = vec { principal \"$(dfx identity get-principal)\" }; mode = variant { AcceptAll }; } })"
    }
  },
  "defaults": { 
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "local2": {
      "bind": "127.0.0.1:8001",
      "type": "ephemeral"
    },
    "staging": {
      "providers": ["https://icp0.io"],
      "type": "persistent"
    },
    "staging2": {
      "providers": ["https://icp0.io"],
      "type": "persistent"
    },
    "ic": {
      "providers": ["https://icp0.io"],
      "type": "persistent"
    }
  },
  "metadata": [
   {
     "name": "candid:service"
   }
  ],
  "output_env_file": ".env",
  "version": 1
}
